@{
    ViewData["Title"] = "Home Page";
}
@model BooksViewModel

<div class="text-center">
    <h1 class="display-4">Bookstore</h1>
    <p></p>
</div>
@foreach (Books b in Model.Books)
{
    <h3>@b.Title</h3>
    <ul>
        <li>Title: @b.Author</li>
        <li>Author: @b.Publisher</li>
        <li>Publisher: @b.Publisher</li>
        <li>ISBN: @b.ISBN</li>
        <li>Classification/Category: @b.Classification</li>
        <li>Number Of Pages: @b.PageCount</li>
        <li>Price: @b.Price</li>
    </ul>

    <br />

    <div page-blah="@Model.PageInfo"></div>
}